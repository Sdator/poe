{
    function 匹配(str) {
        const patt = /">(.*?)</g
        let result = []
        let strTmp = ""
        while ((result = patt.exec(str)) != null) {
            if (result[1]) {
                strTmp += result[1].padEnd(30) + "\t"
            }
        }
        return strTmp
    }
    function 取词缀(词缀) {
        const dom = document.querySelectorAll(".explicitMod")
        let str = ""
        for (const { innerHTML } of dom) {
            str = 匹配(innerHTML)
            if ((str.search(词缀) != -1) && str.search("塑者之") == -1) {
                console.log(str)
            }
        }
    }
    取词缀("暴擊率")
}


